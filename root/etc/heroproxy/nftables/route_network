# IPv6 路由规则
uci add network route6
uci set network.@route6[-1].target='2001:b28:f23d::'
uci set network.@route6[-1].netmask='48'
uci set network.@route6[-1].type='local'
uci set network.@route6[-1].table='200'
uci set network.@route6[-1].interface='lan'

uci add network route6
