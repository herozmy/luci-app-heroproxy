# 添加路由规则标记
uci add network rule
uci set network.@rule[-1].mark='1'
uci set network.@rule[-1].lookup='100'

uci add network rule6
uci set network.@rule6[-1].mark='1'
uci set network.@rule6[-1].lookup='200' 